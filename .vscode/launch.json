    "version": "0.2.0",
    "configurations": [
        {
            "name": "makeOracleCall",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/Debug/net5.0/OracleSample.nef",
            "invocation": {
                "operation": "makeOracleCall",
                "args": []
            },
            "storage": []
        },
        {
            "name": "oracleCallback (checkpoint)",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/Debug/net5.0/OracleSample.nef",
            "invocation": {
                "oracle-response": {
                    "url": "fake:somepath.json",
                    "callback": "oracleCallback",
                    "filter": "Actors[0]",
                    "result-file": "${workspaceFolder}/response.json",
                }
            },
            "checkpoint": "${workspaceFolder}/checkpoints/contract-invoked.neoxp-checkpoint"
        },
        {
            "name": "oracleCallback (no checkpoint)",
            "type": "neo-contract",
            "request": "launch",
            "program": "${workspaceFolder}/src/bin/Debug/net5.0/OracleSample.nef",
            "invocation": {
                "oracle-response": {
                    "url": "fake:somepath.json",
                    "callback": "oracleCallback",
                    "filter": "Actors[1]",
                    "result-file": "${workspaceFolder}/response.json",
                }
            }
        }
    ]
}